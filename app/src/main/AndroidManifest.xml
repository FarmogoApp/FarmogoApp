<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.farmogoapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.main.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.main.AddExploitationActivity"/>
        <activity android:name=".ui.main.ExploitationActivity"/>
        <activity android:name=".ui.main.FarmStatsActivity"/>
        <activity android:name=".ui.main.SeachAnimalsActivity"/>
        <activity android:name=".ui.main.AnimalListActivity"/>
        <activity android:name=".ui.main.AnimalInfoActivity"/>
        <activity android:name=".ui.main.RegisterCow"/>
        <activity android:name=".ui.main.Unregistercow"/>
        <activity android:name=".ui.main.AnimalIncidence"/>
        <activity android:name=".ui.main.RegistrationActivity"/>
    </application>

</manifest>